{
  "name": "bigbrave-frontend-challenge",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "prettier": {
    "withNodeModules": false,
    "useTabs": false,
    "useEditorConfig": false,
    "singleQuote": false,
    "singleAttributePerLine": true,
    "resolveGlobalModules": false,
    "requirePragma": false,
    "requireConfig": false,
    "proseWrap": "preserve",
    "jsxSingleQuote": false,
    "insertPragma": false,
    "ignorePath": ".prettierignore",
    "htmlWhitespaceSensitivity": "css",
    "enableDebugLogs": false,
    "documentSelectors": [],
    "bracketSameLine": false,
    "trailingComma": "es5",
    "tabWidth": 2,
    "quoteProps": "preserve",
    "printWidth": 100,
    "endOfLine": "lf",
    "enable": true,
    "embeddedLanguageFormatting": "auto",
    "arrowParens": "always",
    "bracketSpacing": true,
    "semi": true
  },
  "dependencies": {
    "formik": "^2.2.9",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "eslint-plugin-react": "^7.31.8",
    "postcss": "^8.4.17",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "vite": "^3.1.0"
  }
}
